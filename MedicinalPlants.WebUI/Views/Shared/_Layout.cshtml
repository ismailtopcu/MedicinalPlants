<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("/Views/Layout/HeadPartial.cshtml")
<body id="page-top">

    
    <div id="wrapper">

        @await Html.PartialAsync("/Views/Layout/SidebarPartial.cshtml")
       
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">

                </nav>
                
                @RenderBody()
                

            </div>
            
            @await Html.PartialAsync("/Views/Layout/FooterPartial.cshtml")
            

        </div>
        

    </div>
   
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    @await Html.PartialAsync("/Views/Layout/ScriptPartial.cshtml")

</body>

</html>

    